<div class="page_nav">
<a href="{{currpath}}?mode=summary">summary</a> | <a href="{{currpath}}?mode=shortlog;h={{commit_hash}}">shortlog</a> | <a href="{{currpath}}?mode=log;h={{commit_hash}}">log</a> | <a href="{{currpath}}?mode=commit;h={{commit_hash}}">commit</a> | <a href="{{currpath}}?mode=commitdiff;h={{commit_hash}}">commitdiff</a> | <a href="{{currpath}}">tree</a><br/>
<a href="{{currpath}}/?mode=blobdiff;style=plain;hn={{new.sha1};hp={{old.sha1}}">plain</a></div>
<div>
<a class="title" href="{{currpath}}?mode=commit;h={{commit_hash}}">{{commit.description}}</a>
</div>
<div class="page_path"><b>{{filepath}}</b></div>
<div class="page_body">
<div class="diff_info">blob:<a href="{{currpath}}?h={{old.sha1}}">{{old.sha1}}</a> -&gt; blob:<a href="{{currpath}}?h={{new.sha1}}">{{new.sha1}}</a></div>
{% for line in difflines %}
  <div class="pre {{line.status}}">{{ line.line }}</div>
{% endfor %}
