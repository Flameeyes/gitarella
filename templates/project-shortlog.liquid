<div class="page_nav">
<a href="{{currpath}}?mode=summary">summary</a> | shortlog | <a href="{{currpath}}?mode=log;h={{commit_hash}}">log</a> | <a href="{{currpath}}?mode=commit;h={{commit_hash}}">commit</a> | <a href="{{currpath}}?mode=commitdiff;h={{commit_hash}}">commitdiff</a> | <a href="{{currpath}}">tree</a><br/>
{% if commit_hash != repository.head.sha1 %}<a href="{{currpath}}?mode=shortlog">HEAD</a>
{% else %}HEAD{% endif %}
&sdot;
{% if prev_commits %}<a title="Alt-p" accesskey="p" href="{{currpath}}?mode=shortlog;h={{prev_commits}}">prev</a>
{% else %}prev{% endif %}
&sdot;
{% if more_commits %}<a title="Alt-n" accesskey="n" href="{{currpath}}?mode=shortlog;h={{more_commits}}">next</a>
{% else %}next{% endif %}
<br/>
</div>
<div>
<a class="title" href="{{currpath}}?mode=summary">&nbsp;</a></div>
<table cellspacing="0">
{% specialfor commit in commits %}
  {% if forloop.even %}
    <tr class="light">
  {% else %}
    <tr class="dark">
  {% endif %}
<td><i>{{commit.commit_time | age_str}}</i></td>
<td><i>{{commit.author_name}}</i></td>
<td><a class="list" href="{{currpath}}?mode=commit;h={{commit.sha1}}"><b>{{commit.short_description}}</b></a></td>
<td class="link"><a href="{{currpath}}?mode=commit;h={{commit.sha1}}">commit</a> | <a href="{{currpath}}?mode=commitdiff;h={{commit.sha1}}">commitdiff</a></td>
</tr>
{% endspecialfor %}
