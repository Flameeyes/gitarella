<div class="page_nav">
<a href="{{currpath}}?mode=summary">summary</a> | <a href="{{currpath}}?mode=shortlog;h={{commit_hash}}">shortlog</a> | log | <a href="{{currpath}}?mode=commit;h={{commit_hash}}">commit</a> | <a href="{{currpath}}?mode=commitdiff;h={{commit_hash}}">commitdiff</a> | <a href="{{currpath}}">tree</a><br/>
{% if commit_hash != repository.head.sha1 %}<a href="{{currpath}}?mode=log">HEAD</a>
{% else %}HEAD{% endif %}
&sdot;
{% if prev_commits %}<a title="Alt-p" accesskey="p" href="{{currpath}}?mode=log;h={{prev_commits}}">prev</a>
{% else %}prev{% endif %}
&sdot;
{% if more_commits %}<a title="Alt-n" accesskey="n" href="{{currpath}}?mode=log;h={{more_commits}}">next</a>
{% else %}next{% endif %}
<br/>
</div>
{% for commit in commits %}
<div>
<a class="title" href="{{currpath}}?mode=commit;h={{commit.sha1}}"><span class="age">{{commit.commit_date_age}}</span>{{commit.short_description}}</a>
</div>
<div class="title_text">
<div class="log_link">
<a href="{{currpath}}?mode=commit;h={{commit.sha1}}">commit</a> | <a href="{{currpath}}?mode=commitdiff;h={{commit.sha1}}">commitdiff</a><br/>
</div>
<i>{{commit.author_name}} [{{commit.author_date_str}}]</i><br/>
</div>
<div class="log_body">
{{commit.description}}<br/>
</div>
{% endfor %}
